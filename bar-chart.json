{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Fastest Circuits",
  "width": 600,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/swly777/FIT3179/main/fastestLaps.csv"
  },
  "encoding": {
    "y": {"field": "name", "type": "nominal", "sort": "-x"},
    "x": {"field": "fastest_lap_time", "type": "quantitative", "scale": {"domain": [0, 100]}}
  },
  "layer": [{
    "mark": "bar"
  }, {
    "mark": {
      "type": "text",
      "align": "left",
      "baseline": "middle",
      "dx": 0.5
    },
    "encoding": {
      "text": {"field": "year", "type": "quantitative"}
    }
  }]
}
